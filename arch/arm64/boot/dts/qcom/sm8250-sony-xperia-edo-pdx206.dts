/dts-v1/;

#include "sm8250-sony-xperia-edo.dtsi"

/ {
	model = "Sony Xperia 5 II";
	compatible = "sony,pdx206-generic", "qcom,sm8250";
};

&framebuffer {
	width = <1080>;
	height = <2520>;
	stride = <(1080 * 4)>;
};

&vreg_l2f_1p3 {
	regulator-min-microvolt = <1200000>;
	regulator-max-microvolt = <1200000>;
};
