	};

	sdc1_default: sdc1-default-state {
		clk-pins {
			pins = "sdc1_clk";

			bias-disable;
			drive-strength = <16>;
		};
		cmd-pins {
			pins = "sdc1_cmd";

			bias-pull-up;
			drive-strength = <10>;
		};
		data-pins {
			pins = "sdc1_data";

			bias-pull-up;
			drive-strength = <10>;
		};
	};

	sdc1_sleep: sdc1-sleep-state {
