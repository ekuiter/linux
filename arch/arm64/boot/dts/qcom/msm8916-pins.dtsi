			drive-strength = <12>;
			bias-disable;
		};
		cs-pins {
			pins = "gpio6";
			function = "gpio";

			drive-strength = <16>;
			bias-disable;
			output-high;
		};
	};

	blsp_spi2_sleep: blsp-spi2-sleep-state {
		pins = "gpio4", "gpio5", "gpio6", "gpio7";
