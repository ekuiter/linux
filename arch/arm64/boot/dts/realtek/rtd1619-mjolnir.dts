/dts-v1/;

#include "rtd1619.dtsi"

/ {
	compatible = "realtek,mjolnir", "realtek,rtd1619";
	model = "Realtek Mjolnir EVB";

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x80000000>;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};

	aliases {
		serial0 = &uart0;
		serial1 = &uart1;
		serial2 = &uart2;
	};
};

/* debug console (J1) */
&uart0 {
	status = "okay";
};

/* M.2 slot (CON4) */
&uart1 {
	status = "disabled";
};

/* GPIO connector (T1) */
&uart2 {
	status = "disabled";
};
