/include/ "armada-xp.dtsi"

/ {
	model = "Marvell Armada XP MV78230 SoC";
	compatible = "marvell,armadaxp-mv78230", "marvell,armadaxp", "marvell,armada-370-xp";

	soc {
		pinctrl {
			compatible = "marvell,mv78230-pinctrl";
			reg = <0xd0018000 0x38>;
		};
	};
};
