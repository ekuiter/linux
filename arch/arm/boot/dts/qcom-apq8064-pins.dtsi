
&tlmm_pinmux {
	sdc4_gpios: sdc4-gpios {
		pios {
			pins = "gpio63", "gpio64", "gpio65", "gpio66", "gpio67", "gpio68";
			function = "sdc4";
		};
	};

	ps_hold: ps_hold {
		mux {
			pins = "gpio78";
			function = "ps_hold";
		};
	};

	i2c1_pins: i2c1 {
		mux {
			pins = "gpio20", "gpio21";
			function = "gsbi1";
		};
	};

	i2c3_pins: i2c3 {
		mux {
			pins = "gpio8", "gpio9";
			function = "gsbi3";
		};
	};

	gsbi6_uart_2pins: gsbi6_uart_2pins {
		mux {
			pins = "gpio14", "gpio15";
			function = "gsbi6";
		};
	};

	gsbi6_uart_4pins: gsbi6_uart_4pins {
		mux {
			pins = "gpio14", "gpio15", "gpio16", "gpio17";
			function = "gsbi6";
		};
	};

	gsbi7_uart_2pins: gsbi7_uart_2pins {
		mux {
			pins = "gpio82", "gpio83";
			function = "gsbi7";
		};
	};

	gsbi7_uart_4pins: gsbi7_uart_4pins {
		mux {
			pins = "gpio82", "gpio83", "gpio84", "gpio85";
			function = "gsbi7";
		};
	};
};
