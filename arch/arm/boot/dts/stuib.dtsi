/ {
	soc-u9500 {
		i2c@80110000 {
			bu21013_tp@0x5c {
				compatible = "rhom,bu21013_tp";
				reg = <0x5c>;
				touch-gpio = <&gpio2 20 0x4>;
				avdd-supply = <&ab8500_ldo_aux1_reg>;

				rhom,touch-max-x = <384>;
				rhom,touch-max-y = <704>;
				rhom,flip-y;
			};

			bu21013_tp@0x5d {
				compatible = "rhom,bu21013_tp";
				reg = <0x5d>;
				touch-gpio = <&gpio2 20 0x4>;
				avdd-supply = <&ab8500_ldo_aux1_reg>;

				rhom,touch-max-x = <384>;
				rhom,touch-max-y = <704>;
				rhom,flip-y;
			};
		};
	};
};
