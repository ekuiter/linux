/dts-v1/;
#include "r8a7792.dtsi"

/ {
	model = "Blanche";
	compatible = "renesas,blanche", "renesas,r8a7792";

	aliases {
		serial0 = &scif0;
		serial1 = &scif3;
	};

	chosen {
		bootargs = "ignore_loglevel";
		stdout-path = "serial0:115200n8";
	};

	memory@40000000 {
		device_type = "memory";
		reg = <0 0x40000000 0 0x40000000>;
	};
};

&extal_clk {
	clock-frequency = <20000000>;
};

&scif0 {
	status = "okay";
};

&scif3 {
	status = "okay";
};
